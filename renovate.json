{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 20,
  "packageRules": [
   {
     "matchDatasources": ["docker"],
     "versioning": "loose",
     "allowedVersions": "/\\d+-\\w{40}/"
   }
}
